+
+	<!-- ============================================================= -->
+	<!--  42. BACK MATTER: GLOSSARY                                    -->
+	<!-- ============================================================= -->
+
+	<xsl:template match="glossary">
+
+		<xsl:call-template name="nl-1"/>
