+
+		<xsl:apply-templates/>
+		<xsl:call-template name="nl-1"/>
+
+	</xsl:template>
+
+
+	<!-- ============================================================= -->
+	<!--  Footnote                                                     -->
+	<!-- ============================================================= -->
+
+	<!-- symbol or id is displayed by the first para within the fn     -->
+
+	<xsl:template match="fn">
