+					<xsl:apply-templates/>
+				</span>
+			</xsl:otherwise>
+		</xsl:choose>
+	</xsl:template>
+
+
+	<!-- xlink:href attribute makes a link -->
+
+	<xsl:template match="mailto">
+
+		<xsl:choose>
