+		<table width="60%" cellpadding="2" class="bm">
+			<xsl:call-template name="nl-1"/>
+
+			<xsl:call-template name="table-setup-l-wide"/>
+			<xsl:if test="title">
+				<tr>
+					<th colspan="2" align="left">
+						<b><xsl:apply-templates select="title"/></b>
+					</th>
+				</tr>
