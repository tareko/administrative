+	</xsl:if>
+
+	</xsl:template>
+
+	<!-- ============================================================= -->
+	<!--  40. BACK-MATTER: FOOTNOTE-GROUP and FN                       -->
+	<!-- ============================================================= -->
+
+	<xsl:template match="fn-group">
+
+		<xsl:call-template name="nl-1"/>
+		<xsl:if test="position()>1">
+			<hr class="section-rule"/>
+		</xsl:if>
