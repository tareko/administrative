+
+	</xsl:template>
+
+
+	<!-- ============================================================= -->
+	<!--  46. TITLES: MAIN ARTICLE DIVISIONS                           -->
+	<!-- ============================================================= -->
+
+	<!-- main or top-level divisions -->
+	<xsl:template match="abstract/sec/p">
+		<xsl:apply-templates/>
+	</xsl:template>
+
+	<xsl:template match="abstract/sec">
